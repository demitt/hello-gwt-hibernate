<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <div class="container">
            <div class="content">
                <h1>Login Form</h1>
                <div class="form-divider"></div>
                <div id="login-form">
                    <ul>
                        <li><label>Login</label></li>
                        <li><g:TextBox ui:field="login" /></li>
                        <li><label>Password</label></li>
                        <li><g:PasswordTextBox ui:field="password" /></li>
                        <li><g:Button ui:field="loginButton" styleName="" text="Enter" /></li>
                        <li><g:Button ui:field="btnSmth" text="ЕщеКнопка" /></li>
                    </ul>
                    <g:Label text="{btnSmth.getText}" />
                </div>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>